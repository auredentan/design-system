    "engines": {
        "node": ">= 10.0.0",
        "npm": ">= 3.0.0",
        "yarn": ">= 1.3.0"
    },
    "homepage": "https://github.com/lumapps/design-system",
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "keywords": [
        "AngularJS",
        "Design System",
        "LumApps",
        "LumX",
        "ReactJS"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/lumx.esm.js",
    "name": "@lumx/lumx",
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lumapps/design-system.git"
    },
    "scripts": {
        "build": "npm -s run build:all",
        "build:all": "rm -Rf ./dist/ && npm -s run build:angularjs; npm -s run build:react",
        "build:angularjs": "rm -Rf ./dist/angularjs && webpack --config webpack/angularjs/webpack.umd.js",
        "build:react": "rm -Rf ./dist/react && webpack --config webpack/react/webpack.umd.js",
        "co": "npm -s run commit",
        "commit": "git-cz",
        "cz": "npm -s run commit",
        "eslint:config": "eslint \"webpack/**/*.{js,jsx}\"",
        "eslint:demo": "eslint \"demo/**/*.{js,jsx}\"",
        "eslint:src": "eslint \"src/**/*.{js,jsx}\"",
        "format:demo": "prettier-eslint --prettier-last --write \"demo/**/*.{js,jsx}\"",
        "format:src": "prettier-eslint --prettier-last --write \"src/**/*.{js,jsx}\"",
        "prettier:demo": "prettier --write \"demo/**/*.{js,jsx,html,scss}\"",
        "prettier:src": "prettier --write \"src/**/*.{js,jsx,html,scss}\"",
        "setup": "yarn install; cd ./src/icons/; yarn install; cd -",
        "setup:git-hooks": "node node_modules/husky/bin/uninstall.js && node node_modules/husky/bin/install.js",
        "start:angularjs": "nodemon --watch webpack --ignore webpack/react --ignore webpack/.eslintrc.js node_modules/.bin/webpack-dev-server -- --config webpack/angularjs/webpack.dev.js --hot",
        "start:react": "emon --watch webpack --ignore webpack/angularjs --ignore webpack/.eslintrc.js node_modules/.bin/webpack-dev-server -- --config webpack/react/webpack.dev.js --hot",
        "stylelint:demo": "stylelint \"demo/**/*.scss\"",
        "stylelint:src": "stylelint \"src/**/*.scss\"",
        "test:react": "jest --config jest/react/index.js",
        "test:react:watch": "jest --config jest/react/index.js --watch",
        "tslint:demo": "tslint \"demo/**/*.{ts,tsx}\"",
        "tslint:src": "tslint \"src/**/*.{ts,tsx}\"",
        "webpack": "webpack-dev-server --config webpack/webpack.dev.js",
        "webpack-defaults": "webpack-defaults"
    },
    "version": "0.0.1"
}
