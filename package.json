        "node": ">= 10.0.0",
        "npm": ">= 3.0.0",
        "yarn": ">= 1.3.0"
    },
    "files": [
        "dist"
    ],
    "homepage": "https://github.com/lumapps/design-system",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "keywords": [
        "AngularJS",
        "Design System",
        "LumApps",
        "LumX",
        "ReactJS"
    ],
    "license": "MIT",
    "main": "./dist/lumx.js",
    "name": "@lumx/lumx",
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lumapps/design-system.git"
    },
    "scripts": {
        "build": "yarn run -s build:all",
        "build:all": "yarn run -s build:angularjs; yarn run -s build:react",
        "build:angularjs": "webpack --config webpack/angularjs/webpack.umd.js",
        "build:react": "yarn run -s build:react:commonjs",
        "build:react:commonjs": "webpack --config webpack/react/webpack.commonjs.js",
        "co": "npm -s run commit",
        "commit": "git-cz",
        "cz": "npm -s run commit",
        "format:demo": "prettier-eslint --prettier-last --write \"demo/**/*.{js,jsx}\"",
        "format:src": "prettier-eslint --prettier-last --write \"src/**/*.{js,jsx}\"",
        "prettier:demo": "prettier --write \"demo/**/*.{js,jsx,html,scss}\"",
        "prettier:src": "prettier --write \"src/**/*.{js,jsx,html,scss}\"",
        "setup": "yarn install; cd ./src/icons/; yarn install; cd -",
        "start:angularjs": "webpack-dev-server --config webpack/angularjs/webpack.dev.js --hot",
        "start:react": "webpack-dev-server --config webpack/react/webpack.dev.js --hot",
        "test:react": "jest --config jest/react/index.js",
        "test:react:watch": "jest --config jest/react/index.js --watch",
        "validate-commit": "validate-commit-msg",
        "webpack": "webpack-dev-server --config webpack/webpack.dev.js",
        "webpack-defaults": "webpack-defaults"
    },
    "version": "0.0.1"
}
